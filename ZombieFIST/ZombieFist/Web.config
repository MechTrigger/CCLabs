	</configSections>
	<connectionStrings>
		<!-- Changed 'AttachDbFilename' and 'Initial Catalog' to use the database name 'ZombieFistDb' instead of the long random confusing one it generated -->
		<add name="DefaultConnection" connectionString="Data Source=(LocalDb)\v11.0;AttachDbFilename=|DataDirectory|\ZombieFistDb.mdf;Initial Catalog=ZombieFistDb;Integrated Security=True" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<appSettings>
	</appSettings>
	<system.web>
		<authentication mode="None" />
		<compilation debug="true" targetFramework="4.5" />
